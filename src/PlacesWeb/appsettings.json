{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ServicesConfiguration": {
    "PlacesAPIBaseUrl": "https://localhost:7152/api/places/"
  },
  "Serilog" : {
    "Template" : "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{Level}] {Message}{NewLine}{Exception}",
    "MinimalLevel": {
        "Default": "Information",
        "Override": {
            "Microsoft": "Information",
            "System": "Warning"
        }
    }
  }
}
